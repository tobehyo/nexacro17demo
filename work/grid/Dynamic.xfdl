    <Script type="xscript5.1"><![CDATA[/**
*  nexacro 17 demo
*  @MenuPath    grid > Dynamic
*  @FileName  Dynamic.xfdl 
*  @Creator  presales
*  @CreateDate  2019.10.14
*  @Desction    Define script standards and annotation standards
************** Source modification history ************************************
*  date            Modifier                Description
*******************************************************************************
*  2019.10.14      presales                  Initial creation
*  2020.01.31	   presales					 Bug fixes
*******************************************************************************
*/

/******************************************************************************
 * FORM Variable declaration area
 ******************************************************************************/
this.fv_sTextVal = [];
this.fv_dropFlag;
this.fv_formats1; //variable to store the Grid grdList01 "formats" attribute when form loads
this.fv_formats2; //variable to store the Grid grdList02 "formats" attribute when form loads

/************************************************************************************************
 * FORM EVENT area (onload, onbeforeclose)
 ************************************************************************************************/
 
/**
 * @description Stores the formats information of the two grid components into local variables when
 * the form loads
 * @param {nexacro object} Form the current form object 
 * @param {nexacro event} LoadEventInfo form load event 
 * @returns none
*/
this.Dynamic_onload = function (obj:Form, e:nexacro.LoadEventInfo)
{
	this.fv_formats1 = this.grdList01.formats;
	this.fv_formats2 = this.grdList02.formats;
};

/************************************************************************************************
 * CALLBACK callback processing part (Transaction, Popup)
 ************************************************************************************************/


 /************************************************************************************************
 * CRUD and TRANSACTION service call handling
 ************************************************************************************************/

/************************************************************************************************
 * User FUNCTION area
 ************************************************************************************************/


/************************************************************************************************
 * EVENT area for each COMPONENT
 ************************************************************************************************/

/**
   Occurs when the dragging mouse moves in the component area
 * @description Form ondragmove
 * @param {nexacro Object} Form current form object
 * @param {nexacro Event} DragEventInfo mouse drag event
 * @returns none
*/
this.Dynamic_ondragmove = function (obj:Form, e:nexacro.DragEventInfo)
{
	if(e.clientx < 15) {
		this.fnDragImgClear();
	}
	this.stDragImg.move(e.clientx + 15, e.clienty + 15);
};

/**
   Occurs when dropping a dragged component in the component area
 * @description Form ondrop
 * @param {nexacro Object} Form the current form object
 * @param {nexacro Event} DragEventInfo mouse drag event
*/
this.Dynamic_ondrop = function (obj:Form, e:nexacro.DragEventInfo)
{
	// Processed when it is not a Grid Drop
	var objComp = e.fromobject;

	if (objComp != "[object Grid]") 
	{
		this.fnDragImgClear();
	}
};

/**
   Occurs when dropping a dragged component in the component area
 * @description Form ondrop
 * @param {nexacro Object} Button
 * @param {nexacro Event} ClickEventInfo when button object is clicked
 * @returns none
*/
this.fnComBtn = function(obj:Button, e:nexacro.ClickEventInfo)
{
	switch (obj.name) 
	{
		case "btnInit01":
			// reset
			this.dropFlag = 0;
			this.grdList01.set_formats(this.fv_formats1);
			this.grdList01.set_nodatatext("NO DATA");
			this.dsList01.clearData();
			break;
		case "btnInit02":
			// reset
			this.grdList02.set_formats(this.fv_formats2);
			this.grdList02.set_nodatatext("NO DATA");
			this.dsList02.clearData();
			break;
	}
};

/**
 * @description Grid ondrag
 * @param {nexacro Object} Grid the current grid object 
 * @param {nexacro Event} GridDragEventInfo event related to Drag action in Grid component
*/

this.fnOndrag = function (obj:Grid, e:nexacro.GridDragEventInfo)
{
	switch (obj.name) 
	{
		case "grdColumn":
			// Move a single column
			// Prevent the error that an empty column is generated when the header is clicked
			if (e.row < 0) 
			{
				return;
			}

			this.fv_dropFlag = "single";
			this.stDragImg.set_text(obj.getCellText(e.row, e.cell));
			this.stDragImg.selcol = e.col;
			this.stDragImg.set_visible(true);

			e.userdata = obj.getCellProperty("body", e.cell, "text").toString().replace("bind:", "");

			// Prevent column parsing duplication
			var nCount = this.grdList01.getCellCount("Head");
			var nCount2 = this.grdList02.getCellCount("Head");
			var sCol = this.stDragImg.text;

			if (nCount > 0) 
			{
				for (var j = 0; j < nCount; j++) 
				{
					var varProperty01 = this.grdList01.getCellProperty("Head", j, "text");
					var varProperty02 = this.grdList02.getCellProperty("Head", j, "text");
					//trace("sCol ::" + sCol + ":::: // varProperty:: " + varProperty01);
					if (sCol == varProperty01 || sCol == varProperty02) 
					{
						this.fnDragImgClear();
						return;
					}
				}
			}
			
			return true;
			break;
		case "grdMultiColumn":
			// Move Multi Column
			this.fv_dropFlag = "multi";

			var sColId = this.dsMulticolumn.getColID(e.col);

			if (sColId == "chk") 
			{
				return;
			}
			else 
			{
				var sChk;

				// Return if there is no checked data
				if (this.fv_sTextVal.length == 0 || this.fv_sTextVal == "") 
				{
					return;
				}

				for (var i = 0; i < this.dsMulticolumn.rowcount; i++) 
				{
					sChk = this.dsMulticolumn.getColumn(i, "chk");

					if (sChk == 1) 
					{
						this.stDragImg.set_text(this.fv_sTextVal);
						this.stDragImg.selcol = e.col;
						this.stDragImg.set_visible(true);
					}
				}

				e.userdata = obj.getCellProperty("body", e.cell, "text").toString().replace("bind:", "");

				return true;
			}
			break;
	}
};

/**
 * @description Grid ondrop
 * @param {nexacro Object} Grid current grid object
 * @param {nexacro Event} GridDragEventInfo event related to Drag action in Grid component
 * @returns none
*/
this.dropFlag = 0;

this.fnOndrop = function (obj:Grid, e:nexacro.GridDragEventInfo)
{
	var objDs = obj.getBindDataset();
	// for performance turn off grid redraw event dataset event and turn it on end of script
	obj.set_enableredraw(false);
	objDs.set_enableevent(false);
	if (this.fv_dropFlag == "single") 
	{
		var nColIdx;
		var nCellCnt;
		var sCol;
		var objColumnInfo;
		var nRowCnt1 = this.dsGrid.rowcount;
		var nRowCnt2 = objDs.rowcount;

		/* Create Grid Formats-Start */
		if(nRowCnt2 > 0) {
			obj.appendContentsCol();
			nColIdx = obj.getFormatColCount();
			nCellCnt = obj.getCellCount("Head");
		}else {
			obj.appendContentsRow("body");
			nColIdx = 0;
		}
		
			if(nColIdx > 0) 
			{
				nColIdx -= 1;
			}
		sCol = this.stDragImg.text;
		var colInfo = sCol.toLowerCase().replace(/ /g,"");
		objColumnInfo = this.dsGrid.getColumnInfo(colInfo);
		objDs.addColumn(colInfo, objColumnInfo.type, objColumnInfo.size);
		for (var i = 0; i < nRowCnt1; i++) 
		{
			var sColData = this.dsGrid.getColumn(i, colInfo);

			if (nRowCnt2 == 0) 
			{
				objDs.addRow();
			}
			objDs.setColumn(i, colInfo, sColData);
		}
		obj.setCellProperty("head", nColIdx, "text", sCol);
		obj.setCellProperty("body", nColIdx, "text", "bind:" + colInfo);
		var strFont = obj._getCurrentStyleInheritValue("font");
		var strText = sCol;
		var objFontSize = nexacro.getTextSize( strText, strFont );	
		obj.setFormatColProperty(nColIdx, "size", objFontSize.nx + 50);

		//this.grdList01.setRealRowSize(-1, 30);
		/*  Create Grid Formats-End */

		// Grid Cell Focus processing
		obj.setFocus();
		objDs.set_rowposition(0);
	}
	else 
	{
		this.dropFlag = 0;
		// Multi
		// Initialize Grid Formats
		obj.setFormat("");
		objDs.clearData();

		/* Create Grid Formats - Start */
		var nColIdx = 0;
		var nCellCnt = obj.getCellCount("Head");
		var sCol;
		var objColumnInfo;

		if (nCellCnt == 0) 
		{
			obj.appendContentsRow("head");
			obj.appendContentsRow("body");
		}

		var nRowCnt1 = this.dsGrid.rowcount;

		for (var i = 0; i < this.fv_sTextVal.length; i++) 
		{
			if (nColIdx == i) 
			{
				
			}
			else 
			{
				obj.appendContentsCol();
				//trace("nColIdx : " + nColIdx);
				nColIdx += 1;
			}
			var colInfo = this.fv_sTextVal[i].toLowerCase().replace(/ /g,"");
			objColumnInfo = this.dsGrid.getColumnInfo(colInfo);

			objDs.addColumn(colInfo, objColumnInfo.type, objColumnInfo.size);
			
			for (var j = 0; j < nRowCnt1; j++) 
			{
				var sColData = this.dsGrid.getColumn(j, colInfo);
				var nRowCnt2 = objDs.rowcount;

				if (i == 0) 
				{
					objDs.addRow();
				}
				objDs.setColumn(j, colInfo, sColData);
			}

			obj.setCellProperty("head", nColIdx, "text", this.fv_sTextVal[i]);
			obj.setCellProperty("body", nColIdx, "text", "bind:" + colInfo);
			var strFont = obj._getCurrentStyleInheritValue("font");
			var strText = this.fv_sTextVal[i];
			var objFontSize = nexacro.getTextSize( strText, strFont );			
			obj.setFormatColProperty(nColIdx, "size", objFontSize.nx + 50);
		}

		obj.setRealRowSize(-1, 30);
		/* Create Grid Formats-End   */

		// Grid Cell Focus processing
		obj.setFocus();
		objDs.set_rowposition(0);
	}
	objDs.set_enableevent(true);
	obj.set_enableredraw(true);
	this.fnDragImgClear();
};

/**
 * @description When the header cell of the grid object "grdMultiColumn" is checked or unchecked, it checks 
 * or unchecks every other cell in that column
 * @param {nexacro Object} Grid the current Grid
 * @param {nexacro Event} GridClickEventInfo event that fires when a cell in the Grid is clicked
 * @returns none
*/
this.grdMultiColumn_onheadclick = function (obj:Grid, e:nexacro.GridClickEventInfo)
{
	// All selection, all cancellation processing
	if (e.col == 0) 
	{
		if (this.chkFlag == "1") 
		{
			this.fnBlockUnChk(obj);
			this.chkFlag = "0";
		}
		else 
		{
			this.fnBlockChk(obj);
			this.chkFlag = "1";
		}
	}
};

/**
 * @description  Dataset oncolumnchanged
 * @param {nexacro Object} Dataset
 * @param {nexacro Event} DSColChangeEventInfo event that fires when a column in a DataSet object changes
 * @returns none
*/
this.dsMulticolumn_oncolumnchanged = function (obj:Dataset, e:nexacro.DSColChangeEventInfo)
{
	var nCnt = 0;
	var sChkVal = "";
	this.fv_sTextVal = [];

	for (var i = 0; i < obj.rowcount; i++) 
	{
		sChkVal = obj.getColumn(i, "chk");

		if (sChkVal == 1) 
		{
			this.fv_sTextVal[nCnt] = obj.getColumn(i, "column");
			nCnt += 1;
		}
	}
};

/**
 * @description Drag Image Information Clear
 * @param none
 * @returns none
*/
this.fnDragImgClear = function ()
{
	this.stDragImg.set_visible(false);
	this.stDragImg.filePath = "";
	this.stDragImg.fileName = "";
};

/**
 * @description Grid Check Column All selection processing
 * @param {nexacro Object} Grid object
 * @returns none
*/
this.fnBlockChk = function (objGrid)
{
	var objDs = objGrid.getBindDataset();
	var nCnt = 0;

	for (var i = 0; i < objDs.rowcount; i++) 
	{
		objDs.setColumn(i, "chk", "1");
	}

	objGrid.setCellProperty("Head", 0, "text", "1");
};

/**
 * @description Complete processing of Grid Check Column cancellation
 * @param {nexacro Object} Grid object
 * @returns none
*/
this.fnBlockUnChk = function (objGrid)
{
	var objDs = objGrid.getBindDataset();

	for (var i = 0; i < objDs.rowcount; i++) 
	{
		objDs.setColumn(i, "chk", 0);
	}

	objGrid.setCellProperty("Head", 0, "text", "0");
};

]]></Script>
