/**
* fnMoveAni : Animation Interpolation Function
* @param {String}  menuId	- Menu ID
* @param {String}  menuNm	- Menu name
* @return : N/A
* @example :  
*/
this.fnMoveAni = function (t, b, c, d) 
{
	t /= d/2;
	if (t < 1) return c/2*t*t*t + b;
	t -= 2;
	return c/2*(t*t*t + 2) + b;
};


/**
* fnSetMenu : User-set events
* @param {String}  menuId	- Menu ID
* @param {String}  menuNm	- Menu name
* @return : N/A
* @example :  
*/
this.fnSetMenu = function()
{
	var i;
	var objBtn;
	var nTop = 0;
	var sMenuNm;
	
	this.dsMenu.filter("level==0");
	
	for(i=0;i<this.dsMenu.rowcount;i++)
	{
		sMenuNm = this.dsMenu.getColumn(i, "caption");
		
		objBtn = new Button("btnMenu"+i, 0, nTop, null, 33, 0, null);
		this.divMenu.addChild(objBtn.name, objBtn);
		
		objBtn.show();
		
		objBtn.set_cssclass("btn_cp_basic01");
		objBtn.set_text(sMenuNm);
		objBtn.orgTop = nTop;
		
		objBtn.addEventHandler("onclick", this.btnMenu_onclick, this);
		nTop = objBtn.getOffsetBottom()+2;
	}
	this.dsMenu.filter("");
	this.divMenu.form.btnMenu0.click();
}


/************************************************************************************************
 * EVENT area for each COMPONENT
 ************************************************************************************************/
/**
 * @description Button click event
*/
this.btnMenu_onclick = function(obj, e)
{
	this.dsButtonIdx.setColumn(0, "Idx", obj.name.substr(obj.name.length-1));
	
	this.objTarget = obj;
	var nTop = this.objTarget.getOffsetBottom();
	
	if(this.objTarget_befor!="")
	{
		this.setTimer(1, 10);
	}else
	{
		this.divMenu.form.grdHideMenu.move(5, nTop, null, 0, 5, null);
		this.setTimer(2, 10);
	}
}

/**
 * @description Menu call button
*/
this.btnCall_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.pmnuMenu.trackPopupByComponent(obj, 0, 26);
}

/**
 * @description Grid mouse move event
*/
this.grdMenu_onmousemove = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(this.fv_Row!=e.row)
	{
		if(e.row<0)
		{
			this.divMenu1.set_visible(false);
			return;
		}

		var nRowHeight = nexacro.toNumber(this.divMenu1.form.grdMenu2.getRealRowSize(0));
		var nLeft = obj.getOffsetLeft()+obj.getOffsetWidth();
		var nTop = obj.getOffsetTop()+(e.row*nRowHeight);
		this.fv_Row = e.row;
		this.divMenu1.set_visible(true);
		this.divMenu1.move(nLeft-30, nTop);
		sMENU_CD = this.dsGridMenu.getColumn(this.fv_Row, "MENU_CD");
		this.dsMenu1.filter("UP_MENU_CD=='"+sMENU_CD+"'");
		this.dsGridMenu2.copyData(this.dsMenu1, true);
		this.dsMenu1.filter("");
		
		this.divMenu1.resize(this.divMenu1.getOffsetWidth(), this.divMenu1.form.grdMenu2.getRealRowFullSize() +13);
		
		this.Static00.bringToFront();
		this.grdMenu.bringToFront();
		this.divMenu1.bringToFront();
//		obj.setFocus();
// 		this.killTimer(3);
// 		this.setTimer(3, 2000);
// 		this.PopupDiv00.trackPopup(nLeft, nTop);
	}
}

