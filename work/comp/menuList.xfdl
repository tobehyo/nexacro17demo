};


this.Static00_onclick = function(obj:nexacro.Static,e:nexacro.ClickEventInfo)
{
	this.divMenu1.set_visible(false);
	this.Static00.sendToBack();
};

this.divMenu1_grdMenu2_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	this.divMenu1.set_visible(false);
	this.Static00.sendToBack();
	
};]]></Script>
