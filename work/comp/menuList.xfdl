************** 소스 수정 이력 ***********************************************
*  date            Modifier                Description
*******************************************************************************
*  2019/10/02          presales                  최초 생성 
*******************************************************************************
*/

/************************************************************************************************
 * FORM 변수 선언 영역
 ************************************************************************************************/
this.fv_timerIdx = 0;
this.fv_timerMax = 20;
this.fv_pageHeight = 122;
this.objTarget;
this.objTarget_befor = "";
this.fv_Row;

/************************************************************************************************
 * FORM EVENT 영역(onload, onbeforeclose)
 ************************************************************************************************/
/**
 * @description 화면 load시 초기설정 event
*/
this.work013_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	/*
	var nLevel;
	for(i=0;i<this.Dataset00.rowcount;i++)
	{
		nLevel = this.Dataset00.getColumn(i, "level");
		
		if(nLevel!="0")
		{
			this.grdLeftMenu00.setRealRowSize(i, 0);
		}
	}
	*/
	
	this.fnSetMenu();
}


/**
 * @description 화면 load시 초기설정 event
*/
this.work013_ontimer = function(obj:Form, e:nexacro.TimerEventInfo)
{
	if(e.timerid==1)
	{
		var objTile;
		var nValue = this.fv_pageHeight - this.fnMoveAni(this.fv_timerIdx, 0, this.fv_pageHeight, this.fv_timerMax);
		var nTop;
		
		this.fv_timerIdx++;
		
		for(i=0;i<this.divMenu.form.components.length;i++)
		{
			objTile = this.divMenu.form.components[i];
			
			nTop = nexacro.toNumber(objTile.orgTop)
			
			if(objTile.orgTop>this.objTarget_befor.orgTop)
			{
				nTop = nTop + nValue;
			}
		
			objTile.setOffsetTop(nTop);
		}
		
		this.divMenu.form.grdHideMenu.setOffsetHeight(nValue);
		
		if(this.fv_timerIdx==this.fv_timerMax)
		{
			var nTop = this.objTarget.getOffsetBottom();
			this.divMenu.form.grdHideMenu.move(5, nTop, null, 0, 5, null);
			
			this.fv_timerIdx = 0;
						
			this.killTimer(1);
			this.setTimer(2, 10);
		}
	}
	else if(e.timerid==2)
	{
		var objTile;
		var nValue = this.fnMoveAni(this.fv_timerIdx, 0, this.fv_pageHeight, this.fv_timerMax);
		var nTop;
		
		this.fv_timerIdx++;
		
		for(i=0;i<this.divMenu.form.components.length;i++)
		{
			objTile = this.divMenu.form.components[i];
			
			nTop = nexacro.toNumber(objTile.orgTop)
			
			if(objTile.orgTop>this.objTarget.orgTop)
			{
				nTop = nTop + nValue;
				objTile.setOffsetTop(nTop);
			}
		}
		this.divMenu.form.grdHideMenu.setOffsetHeight(nValue+2);
		if(this.fv_timerIdx==this.fv_timerMax)
		{
			this.fv_timerIdx = 0;
			
			this.objTarget_befor = this.objTarget;
			
			this.killTimer(2);
		}
	}else if(e.timerid==3)
	{
		this.divMenu1.set_visible(false);
		this.killTimer(3);
	}
}

/************************************************************************************************
 * CALLBACK 콜백 처리부분(Transaction, Popup)
 ************************************************************************************************/

 
 /************************************************************************************************
 * CRUD 및 TRANSACTION 서비스 호출 처리
 ************************************************************************************************/

/************************************************************************************************
 * 사용자 FUNCTION 영역
 ************************************************************************************************/
 
/**
* fnMoveAni : 애니메이션 Interpolation 함수
* @param {String}  menuId	- 메뉴 ID
* @param {String}  menuNm	- 메뉴명
* @return : N/A
* @example :  
*/
this.fnMoveAni = function (t, b, c, d) 
{
	t /= d/2;
	if (t < 1) return c/2*t*t*t + b;
	t -= 2;
	return c/2*(t*t*t + 2) + b;
};


/**
* fnSetMenu : 사용자 설정 이벤트
* @param {String}  menuId	- 메뉴 ID
* @param {String}  menuNm	- 메뉴명
* @return : N/A
* @example :  
*/
this.fnSetMenu = function()
{
	var i;
	var objBtn;
	var nTop = 0;
	var sMenuNm;
	
	this.dsMenu.filter("level==0");
	
	for(i=0;i<this.dsMenu.rowcount;i++)
	{
		sMenuNm = this.dsMenu.getColumn(i, "caption");
		
		objBtn = new Button("btnMenu"+i, 0, nTop, null, 33, 0, null);
		this.divMenu.addChild(objBtn.name, objBtn);
		
		objBtn.show();
		
		objBtn.set_cssclass("btn_cp_basic01");
		objBtn.set_text(sMenuNm);
		objBtn.orgTop = nTop;
		
		objBtn.addEventHandler("onclick", this.btnMenu_onclick, this);
		nTop = objBtn.getOffsetBottom()+2;
	}
	this.dsMenu.filter("");
	this.divMenu.form.btnMenu0.click();
}


/************************************************************************************************
 * 각 COMPONENT 별 EVENT 영역
 ************************************************************************************************/
/**
 * @description 버튼 click이벤트
*/
this.btnMenu_onclick = function(obj, e)
{
	this.dsButtonIdx.setColumn(0, "Idx", obj.name.substr(obj.name.length-1));
	
	this.objTarget = obj;
	var nTop = this.objTarget.getOffsetBottom();
	
	if(this.objTarget_befor!="")
	{
		this.setTimer(1, 10);
	}else
	{
		this.divMenu.form.grdHideMenu.move(5, nTop, null, 0, 5, null);
		this.setTimer(2, 10);
	}
}

/**
 * @description 메뉴호출 버튼
*/
this.btnCall_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	this.pmnuMenu.trackPopupByComponent(obj, 0, 26);
}

/**
 * @description 그리드 마우스 move 이벤트
*/
this.grdMenu_onmousemove = function(obj:Grid, e:nexacro.GridMouseEventInfo)
{
	if(this.fv_Row!=e.row)
	{
		if(e.row<0)
		{
			this.divMenu1.set_visible(false);
			return;
		}

		var nRowHeight = nexacro.toNumber(this.divMenu1.form.grdMenu2.getRealRowSize(0));
		var nLeft = obj.getOffsetLeft()+obj.getOffsetWidth();
		var nTop = obj.getOffsetTop()+(e.row*nRowHeight);
		this.fv_Row = e.row;
		this.divMenu1.set_visible(true);
		this.divMenu1.move(nLeft-30, nTop);
		sMENU_CD = this.dsGridMenu.getColumn(this.fv_Row, "MENU_CD");
		this.dsMenu1.filter("UP_MENU_CD=='"+sMENU_CD+"'");
		this.dsGridMenu2.copyData(this.dsMenu1, true);
		this.dsMenu1.filter("");
		
		this.divMenu1.resize(this.divMenu1.getOffsetWidth(), this.divMenu1.form.grdMenu2.getRealRowFullSize() +13);
		
		this.Static00.bringToFront();
		this.grdMenu.bringToFront();
		this.divMenu1.bringToFront();
//		obj.setFocus();
// 		this.killTimer(3);
// 		this.setTimer(3, 2000);
// 		this.PopupDiv00.trackPopup(nLeft, nTop);
	}
}

/**
 * @description 그리드 마우스 killfocus 이벤트
*/
this.grdMenu_onkillfocus = function(obj:nexacro.Grid,e:nexacro.KillFocusEventInfo)
{
	//this.divMenu1.set_visible(false);
