/************************************************************************************************
 * FORM Variable declaration area
 ************************************************************************************************/
this.fv_timerIdx = 0;
this.fv_timerMax = 20;
this.fv_pageHeight = 122;
this.objTarget;
this.objTarget_befor = "";
this.fv_Row;

/************************************************************************************************
 * FORM EVENT domain(onload, onbeforeclose)
 ************************************************************************************************/
/**
 * @description Initial setting event upon screen load
*/
this.work013_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	/*
	var nLevel;
	for(i=0;i<this.Dataset00.rowcount;i++)
