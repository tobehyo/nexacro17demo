/************************************************************************************************
 * User FUNCTION
 ************************************************************************************************/
this.fnCreateNumberAnimation = function()
{
	var arrNumberAnimation = [ 
		this.divBody.form.divMain01.form.staNumber,
		this.divBody.form.divMain02.form.staNumber,
		this.divBody.form.divMain03.form.staNumber,
		this.divBody.form.divMain04.form.staNumber 
	];
	
	// delay each animation
	var delayAnimation = function(idx, objAni, pThis,nodelay) {
		if(nodelay) return objAni.play();
		nexacro._OnceCallbackTimer.callonce(pThis, function() {
			objAni.play();
		},idx*300);
	}	
	var cnt = arrNumberAnimation.length;
	for(var i=0; i<cnt; i++) {
		var sAniId = arrNumberAnimation[i]._unique_id;
		if(this[sAniId]) {
			delayAnimation(i, this[sAniId], this, true);
			continue;
		}
		var targetObj = arrNumberAnimation[i];
		var aniValue = arrNumberAnimation[i].text;
		var itemName = "AniItem00";
		var	objAni = new nexacro.Animation(sAniId, this);
		this.addChild(sAniId, objAni);
		
		objAni.set_duration(1000);
		objAni.set_easing("easeinoutsine");
		objAni.addTarget(itemName , targetObj, "text:[0,"+aniValue+"]");
		objAni.setEventHandler("onrun", this.Animation_onrun, this);
		objAni.addEventHandler("oncomplete",this.Animation_oncomplete, this);
					
		delayAnimation(i, objAni, this);
	}
}

this.Animation_onrun = function(obj:nexacro.Animation,e:nexacro.AnimationEventInfo)
{
	//trace(obj.id);
};

this.Animation_oncomplete = function(obj:nexacro.Animation,e:nexacro.AnimationEventInfo)
{
	// TODO trace(obj.id);
};
