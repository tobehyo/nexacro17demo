/**
 * @description set animation for step using ontimer event
*/

this.Main_ontimer = function(obj:nexacro.Form,e:nexacro.TimerEventInfo)
{
	var nNum = 0;
	
	if(e.timerid == 6666)
	{
		var stepIndex = this.divMainVisual.form.getStepIndex();
		stepIndex = nexacro.toNumber(stepIndex) + 1;
		
		if(stepIndex > 2)
		{
			stepIndex = 0;
		}
		this.divMainVisual.form.setStepIndex(stepIndex);
	}

};

/**
 * @description repeat animation
*/
this.Main_onvscroll = function(obj:nexacro.Form,e:nexacro.ScrollEventInfo)
{
	if(e.pos > 0 && e.pos < 120)
	{
		this.fnCreateNumberAnimation();
	}
	else if(e.pos > 480 && e.pos < 600)
	{
		this.divBody.form.divMain05.form.divChart.form.BasicChart00.runAnimation();
		this.divBody.form.divMain06.form.divChart.form.PieChart00.runAnimation();
	}
	else if(e.pos > 840 && e.pos < 960)
	{
		this.divBody.form.divMain07.form.divChart.form.BasicChart00.runAnimation();
		//this.divBody.form.divMain08.form.divChart.form.GaugeChart01.runAnimation();
	}
};



