 ************************************************************************************************/
this.fnCreateAnimation = function()
{
	var sAniId = "mainAni01";
	var	objAni = new nexacro.Animation( sAniId, this);
		
	this.addChild(sAniId, objAni);
	
	objAni.set_duration( 1000 );
	objAni.set_easing( "easeinoutsine" );
	objAni.addTarget(sAniId , this.divBody.form.divMain01.form.staNumber, "text:999");
	objAni.setEventHandler("onrun", this.Ani_onrun, this);
	//objAni.addEventHandler("oncomplete",this.Animation_oncomplete, this);
	
	objAni.play();
}

this.Ani_onrun = function(obj,e)
{
	this.divBody.form.divMain01.form.staNumber.set_text(nexacro.round(this.divBody.form.divMain01.form.staNumber.text));
}
/************************************************************************************************
