/**
* fnMoveAnimation : 애니메이션 Interpolation 함수
* @param {Number}  t	- Timer Index
* @param {Number}  b	- 0
* @param {Number}  c	- 초기값 ( 높이 )
* @param {Number}  d	- Timer Max
* @return : 위치값
* @example :  
*/
this.fnMoveAnimation = function (t, b, c, d) 
{
	t /= d/2;
	if (t < 1) return c/2*t*t*t + b;
	t -= 2;
	return c/2*(t*t*t + 2) + b;
};

/**
 * @description MegaMenu를 생성한다.
*/
this.fvCnt=1;
this.fvMainImgScrollPos = 0;
this.Main_ontimer = function(obj:nexacro.Form,e:nexacro.TimerEventInfo)
{
	var nNum = 0;
	
	if(e.timerid == 7777)
	{
		nNum = Math.round( this.fnMoveAnimation(this.fvMainImgScrollPos, 0, 1280, this.fvDuration));
		this.divMainVisual.form.hscrollbar.set_pos(nNum);
		this.fvMainImgScrollPos++;
	}
	
	if(e.timerid == 9999)
	{
		this.fvCnt++;
		
		this.setTimer(this.fvCnt,10);
		
		if(this.fvCnt > 3)
		{
			this.killTimer(9999);
		}
	}
	
	if(e.timerid == 1)
	{	
		nNum = Math.round( this.fnMoveAnimation(this.fvStartNum1, 0, this.divBody.form.divMain01.form.staNumber._orgNum, this.fvDuration));
		if(nNum >  this.divBody.form.divMain01.form.staNumber._orgNum)
		{
			this.killTimer(1);
			this.fvStartNum1 = 0;
			return;
		}
		this.divBody.form.divMain01.form.staNumber.set_text(nNum);
		this.fvStartNum1++;
	}	
	else if(e.timerid == 2)
	{
		nNum = Math.round( this.fnMoveAnimation(this.fvStartNum2, 0, this.divBody.form.divMain02.form.staNumber._orgNum, this.fvDuration));
		if(nNum > this.divBody.form.divMain02.form.staNumber._orgNum)
		{
			this.killTimer(2);
			this.fvStartNum2 = 0;
			return;
		}
		this.divBody.form.divMain02.form.staNumber.set_text(nNum);
		this.fvStartNum2++;
	}
	else if(e.timerid == 3)
	{
		nNum = Math.round( this.fnMoveAnimation(this.fvStartNum3, 0, this.divBody.form.divMain03.form.staNumber._orgNum, this.fvDuration));
		if(nNum > this.divBody.form.divMain03.form.staNumber._orgNum)
		{
			this.killTimer(3);
			this.fvStartNum3 = 0;
		}
		this.divBody.form.divMain03.form.staNumber.set_text(nNum);
		this.fvStartNum3++;
	}
	else if(e.timerid == 4)
	{
		nNum = Math.round( this.fnMoveAnimation(this.fvStartNum4, 0, this.divBody.form.divMain04.form.staNumber._orgNum, this.fvDuration));
		if( nNum == this.divBody.form.divMain04.form.staNumber._orgNum)
		{
			this.fvStartNum4 = 0;
			this.killTimer(4,10);
		}
		this.divBody.form.divMain04.form.staNumber.set_text(nNum);
		this.fvStartNum4++;
	}
};

/**
 * @description MegaMenu를 생성한다.
*/
this.Main_onvscroll = function(obj:nexacro.Form,e:nexacro.ScrollEventInfo)
{
	if(e.pos > 0 && e.pos < 120)
	{
		this.setTimer(1,5);
		this.fvCnt=1;
		this.setTimer(9999, 700);
	}
	else if(e.pos > 480 && e.pos < 600)
	{
		this.divBody.form.divMain05.form.divChart.form.BasicChart00.runAnimation();
		this.divBody.form.divMain06.form.divChart.form.PieChart00.runAnimation();
	}
	else if(e.pos > 840 && e.pos < 960)
	{
		this.divBody.form.divMain07.form.divChart.form.BasicChart00.runAnimation();
		//this.divBody.form.divMain08.form.divChart.form.GaugeChart01.runAnimation();
	}
};



